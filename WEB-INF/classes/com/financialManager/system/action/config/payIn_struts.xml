<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">

<struts>


	<package name="payIn_struts" extends="struts-default">
		<action name="toPayIn" class="com.financialManager.system.action.PayInAction"
			method="toPayIn">
			<result>payIn/payIn_add.jsp</result>
		</action>
		<action name="toPayInFind" class="com.financialManager.system.action.PayInAction"
			method="toFind">
			<result>payIn/payIn_find.jsp</result>
		</action>
		<action name="payInGet" class="com.financialManager.system.action.PayInAction"
			method="get">
			<result>/payIn/payIn_update.jsp</result>
			<result name="error">/error/exception.jsp</result>
		</action>
		<action name="payInAdd" class="com.financialManager.system.action.PayInAction"
			method="add">
			<result type="redirectAction">payInFindAll</result>
			<result name="error">/error/exception.jsp</result>
		</action>
		<action name="payInUpdate" class="com.financialManager.system.action.PayInAction"
			method="update">
			<result type="redirectAction">payInFindAll</result>
			<result name="error">/error/exception.jsp</result>
		</action>
		<action name="payInDelete" class="com.financialManager.system.action.PayInAction"
			method="delete">
			<result type="redirectAction">payInFindAll</result>
			<result name="error">/error/exception.jsp</result>
		</action>
		<action name="payInFindAll" class="com.financialManager.system.action.PayInAction"
			method="findAll">
			<result>/payIn/payIn_list.jsp</result>
			<result name="error">/error/exception.jsp</result>
		</action>
		<action name="payInFind" class="com.financialManager.system.action.PayInAction"
			method="find">
			<result>/payIn/payIn_find.jsp</result>
			<result name="error">/error/exception.jsp</result>
		</action>
	</package>
</struts>
